<h1> Please Fill Form </h1>
<%= form_with model: @Blog do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %>
        <br />
    <%= form.label :content %>
    <%= form.text_field :content %>
        <%= form.submit  'Add Blog' %>
<% end %>
        <br />
 <%= link_to 'back to main page', blogs_path %>